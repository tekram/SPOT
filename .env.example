# Render Environment Variables for SPOT Deployment
# Copy these key-value pairs into your Render dashboard under Environment > Environment Variables

NODE_VERSION=16.20.2

# MongoDB Atlas connection string
DATABASE_URL=

# The Blue Alliance API key (https://www.thebluealliance.com/account)
TBA_API_KEY=
TBA_EVENT_KEY=

# MongoDB ObjectId of the event in the events collection
EVENT_NUMBER=

# Google OAuth (optional, leave blank if not using)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# FIRST API (optional, leave blank if not using)
FMS_API_USERNAME=
FMS_API_KEY=
